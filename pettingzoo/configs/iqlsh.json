{
    "use_wandb": true,
    "device": "cpu",
    "algo": "SIQL",
    "env": "simple_stag_v0",
    "max_cycles": 25,
    "max_eps": 500,
    "eval_every": 10,
    "n_eval": 1,
    "seed": 4,
    "shield_alpha": 1.0,
    "shield_file": "default",
    "shield_version": 0,
    "shield_eval_version": 0,
    "shielded_ratio": 1.0,
    "shield_diff": true,
    "shield_eps": 0.1,
    "update_timestep": 1,
    "train_epochs": 5,
    "gamma": 0.99,
    "eps_clip": 0.1,
    "vf_coef": 0.3,
    "entropy_coef": 0.03,
    "lr_actor": 0.001,
    "lr_critic": 0.001,
    "buffer_size": 100,
    "batch_size": 32,
    "lr": 0.001,
    "tau": 0.01,
    "target_update_type": "none",
    "eps_min": 0.05,
    "eps_decay": 0.99975,
    "explore_policy": "softmax",
    "eval_policy": "softmax",
    "on_policy": false
}