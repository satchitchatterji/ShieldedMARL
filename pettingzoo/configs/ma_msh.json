{
    "use_wandb": false,
    "wandb_project_prefix": "ma_msh",
    "device": "cpu",

    "env": "markov_stag_hunt",
    "max_cycles": 200,
    "max_eps": 1000,
    "eval_every": 100,
    "n_eval": 1,
    "seed": -1,

    "shield_alpha": 1.0,
    "shield_version": 7,
    "shield_eval_version": 7,
    "shielded_ratio": 1,
    "shield_diff": true,

    "algo": "SIPPO",
    "update_timestep": 256,
    "train_epochs": 10,
    "gamma": 0.99,
    "eps_clip": 0.1,
    "lr_actor": 0.001,
    "lr_critic": 0.001,
    "vf_coef": 0.3,
    "entropy_coef": 0.01
}