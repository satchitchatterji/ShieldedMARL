{
    "use_wandb": true,
    "wandb_project_prefix": "snellius_base_debug",
    "device": "cpu",

    "env": "markov_stag_hunt",
    "max_cycles": 200,
    "max_eps": 500,
    "eval_every": 50,
    "n_eval": 1,
    "seed": -2,

    "shield_alpha": 1.0,
    "shield_version": 7,
    "shield_eval_version": 7,
    "shielded_ratio": 1,
    "shield_diff": true,

    "algo": "IPPO",
    "gamma": 0.99,
    "update_timestep": 50,
    "train_epochs": 10,
    "eps_clip": 0.1,
    "lr_actor": 0.001,
    "lr_critic": 0.001,
    "vf_coef": 0.3,
    "entropy_coef": 0.01,

    "env_config": {
        "n_agents": 2,
        "grid_size": [10, 10], 
        "stag_move_prob": 0.0, 
        "rewards": [2, 10, -2],
        "n_plants": 2,
        "n_stags": 1,
        "render_mode": "none"
    }

}